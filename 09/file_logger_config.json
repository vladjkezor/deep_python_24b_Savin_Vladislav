{
  "version": 1,
  "formatters": {
    "file_form": {
      "format": "%(asctime)s - %(filename)s - %(levelname)s - %(message)s",
      "datefmt": "%d-%m-%Y %H:%M:%S"
    }
  },
  "handlers": {
    "file_handler": {
      "class": "logging.FileHandler",
      "level": "INFO",
      "formatter": "file_form",
      "filename": "cache.log",
      "mode": "w"
    }
  },
  "loggers": {
    "lru_logger": {
      "level": "DEBUG",
      "handlers": [
        "file_handler"
      ],
      "propagate": false
    }
  }
}